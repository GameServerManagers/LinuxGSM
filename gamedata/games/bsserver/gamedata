# Game Settings File
# Import Engine
fn_import_game_settings engines/source

# https://developer.valvesoftware.com/wiki/Command_Line_Options#Source_Dedicated_Server
fn_parms(){
parms="-autoupdate -strictportbind -ip ${ip} -port ${port} +clientport ${clientport} +tv_port ${tv_port} +map ${map} +servercfgfile ${servercfg} -maxplayers ${maxplayers}"
}

fn_set_game_setting dependencies  "libstdc++.so.6" "4f9c911de2276f988afaf47659a8367f"

fn_set_game_setting settings "appid" "228780"
fn_set_game_setting settings "map" "duel_winter"
fn_set_game_setting settings "game" "berimbau"
fn_set_game_setting settings "gamename" "Blade Symphony"
fn_set_game_setting settings "steamauth" "user"
