# Game Settings File
# Import Engine
fn_import_game_settings engines/source

fn_parms(){
parms="-game pvkii -strictportbind -ip ${ip} -port ${port} +clientport ${clientport} +tv_port ${tv_port} +map ${map} +servercfgfile ${servercfg} -maxplayers ${maxplayers}"
}
fn_set_game_setting settings "appid" "17575"
fn_set_game_setting settings "gamename" "Pirates, Vikings, and Knights II"
fn_set_game_setting settings "map" "bt_island"
fn_set_game_setting settings "maxplayers" "24"
fn_set_game_setting settings "game" "pvkii"
