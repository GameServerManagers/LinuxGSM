# Game Settings File
# Import Engine
fn_import_game_settings engines/source

fn_parms(){
parms="-game bms -strictportbind -ip ${ip} -port ${port} +clientport ${clientport} +tv_port ${tv_port} +map ${map} +servercfgfile ${servercfg} -maxplayers ${maxplayers}"
}
fn_set_game_setting settings "appid" "346680"
fn_set_game_setting settings "game" "bms"
fn_set_game_setting settings "map" "dm_bounce"
fn_set_game_setting settings "gamename" "Black Mesa"
