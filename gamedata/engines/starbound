fn_set_game_setting settings "executable" "./starbound_server"
fn_set_game_setting settings "servercfg" "sbboot.config"
fn_set_game_setting settings "servercfgdir" "\${executabledir}"
fn_set_game_setting settings "gamelogdir" "\${filesdir}/giraffe_storage"
